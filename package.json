{
    "name": "@purinton/vclogbot",
    "type": "module",
    "version": "1.0.1",
    "description": "A modern Discord app built with Node.js, based on the purinton/discord foundation.",
    "main": "vclogbot.mjs",
    "scripts": {
        "start": "node vclogbot.mjs",
        "test": "npx --node-options=\"--experimental-vm-modules --no-warnings\" jest --detectOpenHandles --silent"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/purinton/vclogbot.git"
    },
    "keywords": [
        "vclogbot",
        "nodejs"
    ],
    "author": "Russell Purinton <russell@purinton.us>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/purinton/vclogbot/issues"
    },
    "homepage": "https://github.com/purinton/vclogbot#readme",
    "devDependencies": {
        "jest": "^30.0.0"
    },
    "jest": {
        "testEnvironment": "node",
        "testMatch": [
            "**/?(*.)+(spec|test).[jt]s?(x)",
            "**/?(*.)+(spec|test).mjs",
            "**/?(*.)+(spec|test).cjs"
        ]
    },
    "dependencies": {
        "@purinton/common": "^1.0.14",
        "@purinton/discord": "^1.0.5",
        "@purinton/mysql": "^1.0.7",
        "dotenv": "^16.5.0"
    }
}